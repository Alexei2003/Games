private int[] [] Massive = new int[bots+1][6];

bots-количество танков
[]-количество танков
[i][1]-жив или нет
[i][2]-x
[i][3]-y
[i][4]-направление
[i][5]-огонь да/нет
[i][6]-движение бота

private int[] [] [] Projectail = new int[bots+1][21][5];

[]-количество танков
[i][1]-номер прожектайла
[i][j][1]-есть/нету
[i][j][2]-x
[i][j][3]-y
[i][j][4]-направление
